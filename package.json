{
  "name": "codefuse-ide",
  "version": "0.6.4",
  "description": "CodeFuse AI Native IDE",
  "main": "out/main",
  "scripts": {
    "start": "electron-forge start",
    "start-web": "run-p start:client start:server start:webview",
    "start:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack-web/webpack.browser.config.js --progress --color --watch",
    "start:webview": "webpack-dev-server --config ./build/webpack-web/webpack.webview.config.js",
    "start:server": "cross-env EXT_MODE=js KTLOG_SHOW_DEBUG=1 IDE_DATA_FOLDER_NAME='.codefuse-ide' CODE_WINDOW_CLIENT_ID='CODE_WINDOW_CLIENT_ID' IDE_LOG_HOME=codefuse-log node --inspect=9991 -r ts-node/register -r tsconfig-paths/register ./src/web/node/index.ts",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron-rebuild": "node -r ts-node/register ./build/rebuild.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "release:minor": "npm run release -- --release-as minor",
    "compile:webview": "cross-env NODE_ENV=production webpack --config ./build/webpack-web/webpack.webview.config.js",
    "compile:browser": "webpack --config ./build/webpack-web/webpack.browser.config.js",
    "compile:ext-host": "webpack --config ./build/webpack-web/webpack.ext-host.config.ts",
    "compile:worker-host": "webpack --config ./build/webpack-web/webpack.worker-host.config.ts",
    "compile:node": "webpack --config ./build/webpack-web/webpack.node.config.ts",
    "postinstall": "patch-package && npm run compile:webview && npm run compile:ext-host && npm run compile:worker-host && npm run download:extensions",
    "download:extensions": "node -r ts-node/register -r tsconfig-paths/register ./build/download-extensions.ts",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=5120 run-p compile:browser compile:node compile:worker-host compile:ext-host compile:webview"
  },
  "repository": {
    "type": "git",
    "url": "git@code.alipay.com:cloud-ide/codefuse-ide.git"
  },
  "keywords": [
    "AI",
    "IDE"
  ],
  "author": {
    "name": "CodeFuse"
  },
  "license": "MIT",
